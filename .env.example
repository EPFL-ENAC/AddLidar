
# Application environment (development, production, staging)
ENVIRONMENT=development

# Docker image for LiDAR data processing
DOCKER_IMAGE=ghcr.io/epfl-enac/lidardatamanager:latest

# Local volume path for data storage
DOCKER_VOLUME=/path/to/your/data/folder

# API prefix for all endpoints
API_PREFIX=/api

# Port number for the API server
PORT=8000
